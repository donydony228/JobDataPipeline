# Airflow Configuration
AIRFLOW__CORE__EXECUTOR=LocalExecutor
AIRFLOW__CORE__LOAD_EXAMPLES=false
AIRFLOW__CORE__FERNET_KEY=your-fernet-key-here-32-chars-long
AIRFLOW__WEBSERVER__SECRET_KEY=your-secret-key-here

# Database Configuration
AIRFLOW_DB_URL=postgresql+psycopg2://airflow:airflow@localhost:5432/airflow
DWH_DB_HOST=localhost
DWH_DB_PORT=5433
DWH_DB_NAME=job_data_warehouse
DWH_DB_USER=dwh_user
DWH_DB_PASSWORD=dwh_password

# Supabase Configuration
SUPABASE_DB_HOST=db.xxxxxxxxx.supabase.co
SUPABASE_DB_PORT=5432
SUPABASE_DB_NAME=postgres
SUPABASE_DB_USER=postgres
SUPABASE_DB_PASSWORD=[你的密碼]
SUPABASE_DB_URL=postgresql://postgres:[password]@db.xxxxxxxxx.supabase.co:5432/postgres

# MinIO Configuration
MINIO_ENDPOINT=localhost:9000
MINIO_ACCESS_KEY=minioadmin
MINIO_SECRET_KEY=minioadmin123

# MongoDB Atlas Configuration
MONGODB_ATLAS_URL=mongodb+srv://job_data_user:<password>@job-data-cluster.xxxxx.mongodb.net/?retryWrites=true&w=majority
MONGODB_ATLAS_DB_NAME=job_market_data

# Redis Configuration
REDIS_URL=redis://localhost:6379

# Scraper Configuration
REQUEST_DELAY=2.0
CONCURRENT_REQUESTS=16
MAX_RETRIES=3
RETRY_DELAY=5.0
USE_PROXY=false

# Environment
ENVIRONMENT=development
LOG_LEVEL=INFO