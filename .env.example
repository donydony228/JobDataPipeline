# Airflow Configuration
AIRFLOW__CORE__EXECUTOR=LocalExecutor
AIRFLOW__CORE__LOAD_EXAMPLES=false
AIRFLOW__CORE__FERNET_KEY=your-fernet-key-here-32-chars-long
AIRFLOW__WEBSERVER__SECRET_KEY=your-secret-key-here

# Database Configuration
AIRFLOW_DB_URL=postgresql+psycopg2://airflow:airflow@localhost:5432/airflow
DWH_DB_HOST=localhost
DWH_DB_PORT=5433
DWH_DB_NAME=job_data_warehouse
DWH_DB_USER=dwh_user
DWH_DB_PASSWORD=dwh_password

# MinIO Configuration
MINIO_ENDPOINT=localhost:9000
MINIO_ACCESS_KEY=minioadmin
MINIO_SECRET_KEY=minioadmin123

# MongoDB Configuration
MONGODB_URL=mongodb://admin:admin123@localhost:27017

# Redis Configuration
REDIS_URL=redis://localhost:6379

# Scraper Configuration
REQUEST_DELAY=2.0
CONCURRENT_REQUESTS=16
MAX_RETRIES=3
RETRY_DELAY=5.0
USE_PROXY=false

# Environment
ENVIRONMENT=development
LOG_LEVEL=INFO